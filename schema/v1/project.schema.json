{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/wikimedia/OKAPI/tree/master/schema/v1/project.schema.json",
  "title": "Project",
  "description": "Structured content schema for project",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the project"
    },
    "dbName": {
      "type": "string",
      "description": "Name of the project database"
    },
    "inLanguage": {
      "type": "string",
      "description": "Language of the project"
    },
    "size": {
      "type": "string",
      "description": "Size of the project"
    },
    "url": {
      "type": "string",
      "description": "Project URL"
    }
  },
  "required": [
    "name",
    "dbName",
    "inLanguage",
    "size",
    "url"
  ]
}
